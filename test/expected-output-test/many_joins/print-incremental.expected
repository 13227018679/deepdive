R?(x text).

dd_delta_R?(x text).

dd_new_R?(x text).

S(x text,
  dd_count int).

dd_delta_S(x text,
           dd_count int).

dd_new_S(x text,
         dd_count int).

dd_new_S(x, dd_count) :-
    S(x, dd_count);
    dd_delta_S(x, dd_count).

T(x text,
  f text,
  dd_count int).

dd_delta_T(x text,
           f text,
           dd_count int).

dd_new_T(x text,
         f text,
         dd_count int).

dd_new_T(x, f, dd_count) :-
    T(x, f, dd_count);
    dd_delta_T(x, f, dd_count).

U(x text,
  l text,
  dd_count int).

dd_delta_U(x text,
           l text,
           dd_count int).

dd_new_U(x text,
         l text,
         dd_count int).

dd_new_U(x, l, dd_count) :-
    U(x, l, dd_count);
    dd_delta_U(x, l, dd_count).

dd_delta_R(x) :-
    dd_delta_S(x),
    T(x, f),
    U(x, l);
    dd_new_S(x),
    dd_delta_T(x, f),
    U(x, l);
    dd_new_S(x),
    dd_new_T(x, f),
    dd_delta_U(x, l).

